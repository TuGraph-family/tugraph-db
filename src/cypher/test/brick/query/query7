MATCH (t:vertex {uri:'brick$VAV'})<-[:rdf$type]-(vav)-[:bf$hasPoint]->(tempsensor)-[:rdf$type]->()-[:rdfs$subClassOf*0..]->({uri:'brick$Temperature_Sensor'}),(vav)-[:bf$hasPoint]->(valvesensor)-[:rdf$type]->()-[:rdfs$subClassOf*0..]->({uri:'brick$Valve_Command'}),(vav)-[:bf$hasPoint]->(setpoint)-[:rdf$type]->()-[:rdfs$subClassOf*0..]->({uri:'brick$Zone_Temperature_Setpoint'}),(tempsensor)-[:bf$isLocatedIn]->(room)-[:rdf$type]->({uri:'brick$Room'}) RETURN vav.uri, room.uri, tempsensor.uri, valvesensor.uri, setpoint.uri
