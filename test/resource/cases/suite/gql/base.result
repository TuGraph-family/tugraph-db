MATCH (n:Person{name:'Michael Redgrave'})
RETURN n.birthyear+3 as nid;
[{"nid":1911}]

MATCH (n:Person{name:'Michael Redgrave'})
RETURN n.birthyear >= 1909 as nid;
[{"nid":false}]

MATCH (n:Person{name:'Michael Redgrave'})-[]-(m)
RETURN n.name as n, m.name;
