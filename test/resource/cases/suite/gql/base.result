MATCH (n:Person) WHERE n.birthyear < 1970
RETURN
    CASE WHEN n.birthyear < 1950 THEN 0 ELSE 1 END AS n1,
    n.birthyear AS n2
NEXT
RETURN
    n1, max(n2)+2*3 AS n2;
[{"n1":1,"n2":1971.0},{"n1":0,"n2":1945.0}]

