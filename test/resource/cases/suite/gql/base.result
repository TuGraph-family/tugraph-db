MATCH (n:Person) WHERE n.birthyear < 1970
RETURN
    max(n.birthyear+1) as max0,
    sum(n.birthyear+1) as sum1,
    sum(n.birthyear) as sum2,
    count(n) as num;
[{"max0":1966.0,"num":11,"sum1":21274.0,"sum2":21263.0}]

